{% extends 'backend/base/index.html.twig' %}


{% block stylesheets %}

    <link type="text/css" href="{{ asset('backend/plugins/comentario/editor.css')}}" rel="stylesheet"/>
    <link href="{{ asset('backend/plugins/jquery-file-upload/blueimp-gallery/blueimp-gallery.min.css" rel="stylesheet')}}" />
    <link href="{{ asset('backend/plugins/jquery-file-upload/css/jquery.fileupload.css" rel="stylesheet')}}" />
    <link href="{{ asset('backend/plugins/jquery-file-upload/css/jquery.fileupload-ui.css" rel="stylesheet')}}" />    
{% endblock %}





{% block contenido %}


    {% include 'backend/includes/alert.messages.html.twig'  %}
    <!-- begin breadcrumb -->
    <ol class="breadcrumb pull-right">
        <li><a href="javascript:;">Home</a></li>
        <li class="active">Dashboard</li>
    </ol>
    <!-- end breadcrumb -->
    <!-- begin page-header -->
    <h1 class="page-header">Crear Usuario Administrativo <small> </small></h1>
    <form id="fileupload" action="{{ path('backend_usuarioAdministrativo_modificarEjecutar') }}" method="POST" enctype="multipart/form-data">
        <div class="col-md-12 ui-sortable">
            <div class="panel panel-inverse">
                <div class="panel-heading">
                    <div class="panel-heading-btn">
                        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                    </div>
                    <h4 class="panel-title">Crear Usuario administrativo</h4>
                </div>
                <div class="panel-body">
                   
                    <input class="form-control hidden" type="text" placeholder="Titulo" name="profesionalId" value="{{ profesional.usuId}}">     
                    <h4 class="m-t-0">Nombres</h4>
                    <input class="form-control" type="text" placeholder="Titulo" name="profesionalNombre" value="{{ profesional.usuNombre}}">   
                    <h4 class="m-t-0">Apellidos</h4>
                    <input class="form-control" type="text" placeholder="Titulo" name="profesionalApellido" value="{{ profesional.usuApellido}}">                    
                    <h4 class="m-t-0">Email</h4>
                    <input class="form-control" type="text" placeholder="Titulo" name="profesionalEmail" value="{{ profesional.usuEmail}}"> 
                    <h4 class="m-t-0">Rut</h4>
                    <input class="form-control" type="text" placeholder="Titulo" name="profesionalRut" value="{{ profesional.usuRut}}">  
                    <h4 class="m-t-0">Clave Antigua</h4>
                    <input class="form-control" type="password" placeholder="Titulo" name="profesionalClaveAntigua"> 
                     <h4 class="m-t-0">Clave</h4>
                    <input class="form-control" type="password" placeholder="Titulo" name="profesionalClave1">                    
                    <h4 class="m-t-0">Repita la Clave</h4>
                    <input class="form-control" type="password" placeholder="Titulo" name="profesionalClave2">                    
           
                </div>
                <div class="panel-footer">
                    <input id="btAgregar" type="submit" class="btn btn-success" value="Publicar entrada">                
                </div>
            </div>                               
        </div>

    </form>


{% endblock %}


{% block javascripts %}


    <script src="{{ asset('backend/plugins/jquery-file-upload/js/vendor/jquery.ui.widget.js')}}"></script>
    <script src="{{ asset('backend/plugins/jquery-file-upload/js/vendor/tmpl.min.js')}}"></script>
